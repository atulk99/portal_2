<h1>Edit Booking</h1>
<%= form_with model:@booking do |f|%>

<div>
  <%=f.label "start"%><br>
  <%=f.date_field :start%>
</div>

<div>
  <%=f.label "end"%><br>
  <%=f.date_field :end%>
</div>

<div>
  <%=f.label "price"%><br>
  <%=f.number_field :price%>
</div>

<div>
  <%=f.label "property"%><br>
  <%=f.select :property_id,Property.all.pluck(:name, :id)%>
</div>

<div>
  <%=f.submit%>
</div>

<%end%>

<%= link_to "Back to bookings", bookings_path %>
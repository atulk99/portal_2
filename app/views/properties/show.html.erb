<div>
	 <table class="table">
	 	<thead>
  		 <tr>
  		  	<th>Property Name</th>
  		  	<th>Address</th>
  		  	<th>Latitude</th>
  		  	<th>Longitude</th>
  		  	<th colspan="3">Operations</th>
  		  	
  		 </tr>
  		</thead>
  		<tbody>
  		 <tr>
   		 	<td><%= @property.name %></td>
			<td><%= @property.address %></td>
			<td><%= @property.latitude %></td>

			<td><%= @property.longitude %></td>
			<td><%=link_to "Edit",edit_property_path(@property.id)%></td>
			<td><%= link_to "Delete",property_path(@property.id),data: { turbo_method: :delete, turbo_confirm: "Are you sure?" }%></td>
			<td><%= link_to 'Show Bookings',bookings_property_path(@property)%></td>

			

  		 </tr>
  		</tbody>
	 </table>

	 <button type="button" class="btn btn-outline-primary">
	 	<%= link_to "Back to properties", properties_path %>
	 </button>
</div>
	
